#include <stdio.h>



int main() {

  // Write C code here

  int car;

  printf("car num:");

  scanf("%d",&car);

  if(243%car==0){

    printf("road a\n");

  }

  else if(135%car==0){

    printf("road b\n");

  }

  else if(567%car==0){

    printf("road c\n");

  }

  else{

    printf("no road\n");

  }

  return 0;

}  









int main() {

   int a=243;

  int b=136;

  int c=567;

  int car=17;

  if(a%17==0)

  {

   printf("cars in road a",a);

  }

   else if(b%17==0)

{

  printf("cars in road b",b);

}

   else if(c%17==0)

{

  printf("cars in road c",c);

}

else

{

  printf("no road");

}

}











