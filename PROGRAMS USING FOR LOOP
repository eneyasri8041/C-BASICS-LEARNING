#include <stdio.h>

int main() {
    for (int i=5;i>=0;i--)
    printf("developer\n");

    return 0;
}



#include <stdio.h>

int main() {
    for (int i=1;i<=5;i++)
    {
    printf("%d",i);
}
    return 0;
}



#include <stdio.h>

int main() {
    int n=5;
    char ch='a';
    for (int i=1;i<=n;i++)
    {
    printf("%c\n",ch);
    
}
    return 0;
}

//abcde

#include <stdio.h>



int main()

{

  int n=5;

  char ch='a';

  for(int i=1;i<=n;i++)

  {

    printf("%c",ch);

    ch++;

  }

}

//XOXOX


#include <stdio.h>



int main()

{

  int n=5;
  for(int i=0;i<n;i++)
  {
      if(i%2==0)
{
    printf("X");
}
else
  {

    printf("0");
  }

  }

}

//a*b*c


#include <stdio.h>



int main()

{

  int n=5;
  char ch='a';
  for(int i=0;i<n;i++)
  {
      if(i%2==0)
{
    printf("%c",ch);
    ch++;
}
else
  {

    printf("*");
  }

  }

}


